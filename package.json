{
  "name": "dockerc",
  "version": "1.0.0",
  "description": "Web application for docker-compose sharing",
  "scripts": {
    "start": "./node_modules/.bin/tsc && node ./dist/server/server.js",
    "dev": "export $(cat .env | xargs) && ./node_modules/.bin/tsc && rm -r ./dist/public/* && rm -r ./dist/views/* && cp -r public/* ./dist/public/ && cp -r views/* ./dist/views/ && node ./dist/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamcdon/dockerc.git"
  },
  "author": "Jared McDonald",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamcdon/dockerc/issues"
  },
  "homepage": "https://github.com/jamcdon/dockerc#readme",
  "dependencies": {
    "express": "^4.18.1",
    "minio": "^7.0.29",
    "mongoose": "^6.4.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "pug": "^3.0.2",
    "randpix": "^1.1.1",
    "sequelize": "^6.21.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/minio": "^7.0.13",
    "@types/node": "^18.0.5",
    "@types/pug": "^2.0.6",
    "typescript": "^4.7.4"
  }
}
